<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\css\login.css" />
    <Content Remove="wwwroot\css\shop.css" />
    <Content Remove="wwwroot\js\audit.js" />
    <Content Remove="wwwroot\js\auth.js" />
    <Content Remove="wwwroot\js\owner.js" />
    <Content Remove="wwwroot\js\ownings.js" />
    <Content Remove="wwwroot\js\product.js" />
    <Content Remove="wwwroot\js\sales.js" />
    <Content Remove="wwwroot\js\shop.js" />
    <Content Remove="wwwroot\js\warehouse.js" />
